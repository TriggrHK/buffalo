@{
}
@model IEnumerable<string>
<div class="accordion-item mt-2">
    <h2 class="accordion-header" id="heading9">
        <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapse9" aria-expanded="true" aria-controls="collapse9">
            <h5>Wrapping</h5>
            <i class="bi bi-chevron-down mx-auto"></i>
        </button>
    </h2>
    <div id="collapse9" class="accordion-collapse collapse" aria-labelledby="heading9" data-bs-parent="#accordionExample">
        <div class="accordion-body d-grid gap-2">
            @foreach (string t in Model)
            {
                <a asp-controller="Home" asp-action="Burials" asp-route-WrappingFilter="@t" asp-route-pageNum="1"
                   class="btn @(ViewBag.SelectedWrapping == t ? "btn-primary" : "btn-outline-secondary")">@t</a>
            }
        </div>
    </div>
</div>


